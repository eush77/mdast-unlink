{
  "name": "remark-unlink",
  "version": "2.0.1",
  "description": "Remove all links, references and definitions",
  "license": "MIT",
  "keywords": [
    "definitions",
    "filter",
    "images",
    "links",
    "mdast",
    "plaintext",
    "references",
    "remark",
    "remove",
    "text"
  ],
  "repository": "eush77/remark-unlink",
  "bugs": "https://github.com/eush77/remark-unlink/issues",
  "author": "Eugene Sharygin <eush77@gmail.com>",
  "contributors": [
    "Eugene Sharygin <eush77@gmail.com>",
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "files": [
    "index.js"
  ],
  "dependencies": {
    "mdast-squeeze-paragraphs": "^3.0.0",
    "unist-util-parents": "^1.0.0",
    "unist-util-select": "^1.0.0"
  },
  "devDependencies": {
    "prettier": "^1.14.2",
    "remark": "^9.0.0",
    "remark-cli": "^5.0.0",
    "remark-preset-wooorm": "^4.0.0",
    "tape": "^4.2.0",
    "xo": "^0.22.0"
  },
  "scripts": {
    "format": "remark *.md -qfo && prettier --write \"**/*.js\" && xo --fix",
    "test-api": "node test/test",
    "test": "npm run format && npm run test-api"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "xo": {
    "prettier": true,
    "esnext": false
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  }
}
